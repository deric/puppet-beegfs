{
  "name": "deric-beegfs",
  "version": "0.6.1",
  "author": "deric",
  "summary": "Manage BeeGFS parallel filesystem installations",
  "license": "Apache-2.0",
  "source": "https://github.com/deric/puppet-beegfs",
  "project_page": "https://github.com/deric/puppet-beegfs",
  "issues_url": "https://github.com/deric/puppet-beegfs/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 2.4.0 < 7.0.0"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 3.0.0 < 7.0.0"
    },
    {
      "name": "puppetlabs-concat",
      "version_requirement": ">= 1.0.0 < 6.0.0"
    },
    {
